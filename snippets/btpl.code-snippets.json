{
	"Btpl Block": {
		"prefix": "{%",
		"body": ["{% $1 %}"],
		"description": "Btpl block {% %}"
	},
	"Btpl If": {
		"prefix": "if",
		"body": ["{% if@$1 %}$2{% end@ %}"],
		"description": "Btpl block {% %}"
	},
	"Btpl For": {
		"prefix": "for",
		"body": ["{% for@$1 %}$2{% end@ %}"],
		"description": "Btpl block {% %}"
	},
	"Btpl end": {
		"prefix": "end",
		"body": ["{% end@ %}$1"],
		"description": "Btpl block {% %}"
	}
}
